{
    "manifest_version":2,
    "name": "SecureWay",
    "description":"Secure your browsing experience",
    "version":"1.0",
    "background":{
        "scripts":["background.js"]
    },
    "content_scripts": [
        {
          "matches": [
            "<all_urls>"
          ],
          "js": ["jquery-3.1.1.min.js","content.js"]
        }
     ],
    "browser_action":{
        "default_popup":"popup.html"
    },
    "permissions":[
        "bookmarks", "tabs"
    ]
}